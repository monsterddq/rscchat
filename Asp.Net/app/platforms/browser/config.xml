<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.e-talk" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>E Talk</name>
    <description>
      A Chat App from E Talk
    </description>
    <author email="doanquocthai.it@gmail.com" href="https://www.linkedin.com/in/doan-quoc-thai-a3733514b/">
        DJDD
    </author>
    <content src="login.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
